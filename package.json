{
  "name": "generator-jaicp",
  "version": "1.0.0",
  "description": "Get up and running with your JAICP projects quickly",
  "author": {
    "name": "Konstantin Sipunin",
    "email": "k.sipunin@just-ai.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "bot",
    "chatbot",
    "jaicp",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^3.1.0",
    "jest": "^26.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.8.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "just-ai/generator-jaicp",
  "license": "Apache-2.0"
}
